[{"title": "Canada prepares for another dreaded megafire season", "link": "https://www.lemonde.fr/en/environment/article/2024/03/19/canada-preparing-for-another-dreaded-megafire-season_6635915_114.html", "thumbnail": "https://img.lemde.fr/2024/03/18/360/0/4608/2304/644/322/60/0/d8635ab_1710775133064-000-34jz6dh.jpg", "date_published": "2024-03-19T22:28:32+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "In 2023, the North American country experienced its most severe forest fire season on record. The persistence of fires underground, mild winter temperatures and insufficient snowfall have led to fears of a similar scenario this year."}, {"title": "US Supreme Court allows Texas law enabling police to arrest undocumented migrants", "link": "https://www.lemonde.fr/en/immigration/article/2024/03/19/us-supreme-court-allows-texas-law-enabling-police-to-arrest-undocumented-migrants_6635849_144.html", "thumbnail": "https://img.lemde.fr/2024/03/19/511/0/3063/1531/644/322/60/0/862fa53_5957372-01-06.jpg", "date_published": "2024-03-19T21:49:18+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "The Texas immigration law dispute is among several legal clashes between Texas and the Biden administration regarding border patrol and preventing illegal crossings at the Texas-Mexico border."}, {"title": "Behind the French dream of entrepreneurship, the reality of bosses earning below the minimum wage", "link": "https://www.lemonde.fr/en/economy/article/2024/03/19/behind-the-french-dream-of-entrepreneurship-the-reality-of-bosses-earning-below-the-minimum-wage_6635649_19.html", "thumbnail": "https://img.lemde.fr/2024/03/18/318/0/4500/2250/644/322/60/0/712158e_1710749983872-240222ch-7768.jpg", "date_published": "2024-03-19T19:40:34+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "One in five small business owners pays themselves less than the minimum wage. The Covid-19 crisis, which led to the collapse of some businesses, has accelerated this trend."}, {"title": "Access to abortion in France remains fraught with obstacles", "link": "https://www.lemonde.fr/en/opinion/article/2024/03/19/access-to-abortion-in-france-remains-fraught-with-obstacles_6635616_23.html", "thumbnail": "https://img.lemde.fr/2024/02/28/439/0/5130/2565/644/322/60/0/e414120_1709137029305-144685.jpg", "date_published": "2024-03-19T19:30:17+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "Despite the March 4 enshrining of the 'guaranteed freedom' of abortion in the Constitution, there are still obstacles holding back its access."}, {"title": "Paris 2024: Russian and Belarusian athletes excluded from participating in opening ceremony", "link": "https://www.lemonde.fr/en/sports/article/2024/03/19/paris-2024-russian-and-belarusian-athletes-excluded-from-participating-in-opening-ceremony_6635484_9.html", "thumbnail": "https://img.lemde.fr/2024/03/19/292/0/3497/1748/644/322/60/0/614e8ef_5956614-01-06.jpg", "date_published": "2024-03-19T18:06:56+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "Announcing their decision on Tuesday, the IOC said these athletes competing under a neutral flag will be able 'to experience the event.'"}, {"title": "French lawmakers move to probe Polynesia nuclear tests", "link": "https://www.lemonde.fr/en/france/article/2024/03/19/french-lawmakers-move-to-probe-polynesia-nuclear-tests_6635352_7.html", "thumbnail": "https://img.lemde.fr/2024/03/19/737/0/7658/3829/644/322/60/0/603c774_1710868352972-311e209-5311058-01-06.jpg", "date_published": "2024-03-19T17:19:33+00:00", "dead": false, "artist": null, "page_rating": 100, "comments": [], "language": "en", "page_rating_visits": 0, "source_obj__id": 189, "page_rating_contents": 100, "vote": 0, "page_rating_votes": 0, "permanent": false, "age": null, "tags": [], "bookmarked": false, "user": null, "source": "https://www.lemonde.fr/en/rss/une.xml", "album": "", "description": "Paris in 2010 acknowledged the health and environmental impacts of the hundreds of bombs detonated in the region, the last as recently as 1996."}, {"title": "Annual energy conference shows oil isn't going away any time soon", "link": "https://www.lemonde.fr/en/environment/article/2024/03/19/annual-energy-conference-shows-oil-isn-t-going-away-any-time-soon_6635285_114.html", "thumbnail": "https://img.lemde.fr/2024/03/19/603/0/7212/3606/644/322/60/0/14750a2_1710833255963-000-1qq00j.jpg", "date_published": "2024-03-19T17:04:28+00:00", "dead": false, "artist": nul